{
    "urls" : [
        "http://theoatmeal.com/comics_pg/page:1"    ],
    "mapping" : {
        "extractor" : "dom_array",
        "path" : "a.arrow_right[href*=comics]",
        "attr" : "href",
        "prefix" : "http://www.theoatmeal.com",
        "follow" : true,
        "mapping" : {
            "extractor" : "dom_object",
            "keys" : {
                "title" : {
                    "extractor" : "dom_string",
                    "path" : "title",
                    "functions" : [
                        {
                            "function" : "split",
                            "arguments" : ["-"]
                        },
                        {
                            "function" : "shift"
                        } 
                    ]
                },
                "data" : {
                    "try" : 
                    [
                        {
                            "extractor" : "dom_string",
                            "path" : "div#comic p.pad img",
                            "attr" : "src"
                        },
                        {
                            "extractor" : "dom_string",
                            "path" : "iframe[src*=youtube]",
                            "attr" : "src",
                            "functions" : [
                                {       
                                    "function" : "replace",
                                    "arguments" : ["//",""]
                                }
                            ]
                        }
                    ]
                }
            }
        }
    }
} 
