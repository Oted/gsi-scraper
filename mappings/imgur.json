        "http://imgur.com/gallery/hot/viral/page/0/hit?scrolled&set=2",
        "http://imgur.com/gallery/hot/viral/page/0/hit?scrolled&set=3",
        "http://imgur.com/gallery/hot/viral/page/1/hit?scrolled&set=0",
        "http://imgur.com/gallery/hot/viral/page/1/hit?scrolled&set=1",
        "http://imgur.com/gallery/hot/viral/page/1/hit?scrolled&set=2",
        "http://imgur.com/gallery/hot/viral/page/1/hit?scrolled&set=3",
        "http://imgur.com/gallery/new/viral/page/0/hit?scrolled&set=0",
        "http://imgur.com/gallery/new/viral/page/0/hit?scrolled&set=1",
        "http://imgur.com/gallery/new/viral/page/0/hit?scrolled&set=2",
        "http://imgur.com/gallery/new/viral/page/0/hit?scrolled&set=3",
        "http://imgur.com/gallery/new/viral/page/1/hit?scrolled&set=0",
        "http://imgur.com/gallery/new/viral/page/1/hit?scrolled&set=1",
        "http://imgur.com/gallery/new/viral/page/1/hit?scrolled&set=2",
        "http://imgur.com/gallery/new/viral/page/1/hit?scrolled&set=3"
    ],
    "mapping" : {
        "extractor" : "dom_array",
        "path" : "div.post a[href*=gallery]",
        "attr" : "href",
        "prefix" : "http://imgur.com",
        "follow" : true,
        "mapping" : {
            "extractor" : "dom_object",
            "keys" : {
                "title" : {
                    "try" : [
                        {
                            "extractor" : "dom_string",
                            "path" : "#image-title"
                        }
                    ]
                },
                "data" : {
                    "try" : 
                    [
                        {   
                            "extractor" : "dom_string",
                            "path" : "a.zoom img",
                            "attr" : "src",
                            "prefix" : "http:"
                        },
                        {
                            "extractor" : "dom_string",
                            "path" : "div#image div a img",
                            "attr" : "src"
                        },
                        {
                            "extractor" : "dom_string",
                            "path" : "div#image div img",
                            "attr" : "src"
                        },
                        {
                            "extractor" : "dom_script",
                            "start_string" : "var videoItem =",
                            "mapping" : {
                                "type" : "string",
                                "path" : "gifUrl"
                            }
                        }
                    ]
                }
            }
        }
    }
}
